    "keywords": [
        "dev4traders",
        "nj4x"
    ],
    "homepage": "https://github.com/dev4traders/nj4x-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail",
            "email": "mikha.dev@gmail.com",
