    "require": {
        "php": "^8.1",
        "ext-soap": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.9",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "D4T\\Nj4x\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D4T\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
